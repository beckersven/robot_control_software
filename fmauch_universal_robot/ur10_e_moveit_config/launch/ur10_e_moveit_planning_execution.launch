<launch>
  <arg name="sim" default="false" />
  <arg name="limited" default="false"/>
  <arg name="debug" default="false" />
  <arg name="model_postfix" default="_NONE"/>
  <arg name="fake_execution" default="false"/>
    
  <!-- Remap follow_joint_trajectory -->
  <remap if="$(arg sim)" from="/follow_joint_trajectory" to="/scaled_pos_joint_traj_controller/follow_joint_trajectory"/>
  
  <!-- Launch moveit -->
  <include file="$(find ur10_e_moveit_config)/launch/move_group.launch">
    <arg name="limited" default="$(arg limited)"/>
    <arg name="debug" default="$(arg debug)" />
    <arg name="fake_execution" default="$(arg fake_execution)"/>
    <arg name="model_postfix" default="$(arg model_postfix)"/>
  </include>
</launch>
